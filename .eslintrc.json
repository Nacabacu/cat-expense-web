{
  "overrides": [
    {
      "files": [
        "**/*.{js,mjs,cjs,ts,jsx,tsx}"
      ],
      "languageOptions": {
        "globals": {
          "browser": true
        }
      },
      "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended"
      ],
    }
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}